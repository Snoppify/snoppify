{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        // "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "server-dist/",
        "baseUrl": ".",
        "allowJs": true,
        "paths": {
            "*": [
                "node_modules/*",
                "src/types/*"
            ]
        }
    },
    "include": [
        "server/**/*"
    ]
}
{
  "name": "snoppify2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run server",
    "build": "npm run build-client-prod && npm run build-server",
    "serve": "npx vue-cli-service build --mode development --watch --port 3001",
    "build-client": "npx vue-cli-service build --mode development --dest dist",
    "build-client-prod": "npx vue-cli-service build --dest dist",
    "lint": "vue-cli-service lint",
    "test": "jest",
    "build-server": "tsc --build tsconfig.server.json",
    "server": "node server-dist/",
    "watch-server": "npm-watch server",
    "start-server": "concurrently \"npm run build-server -- --watch\" \"npm run watch-server\"",
    "start-electron": "tsc -p electron/ && cp electron/index.html electron-dist/ && electron ./electron-dist/main.js",
    "start-app": "concurrently \"npm run start-server\" \"npm run build -- --watch\"",
    "prepare": "husky install"
  },
  "watch": {
    "server": {
      "patterns": [
        "server-dist",
        "snoppify-config.js",
        ".env"
      ]
    },
    "quiet": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mold/snoppify.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mold/snoppify/issues"
  },
  "homepage": "https://github.com/mold/snoppify#readme",
  "dependencies": {
    "app-root-path": "^2.1.0",
    "axios": "^0.19.2",
    "body-parser": "^1.18.3",
    "connect-loki": "^1.2.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.3",
    "express-debug": "^1.1.1",
    "express-history-api-fallback": "^2.2.1",
    "express-session": "^1.17.2",
    "express-socket.io-session": "^1.3.5",
    "fingerprintjs2": "^1.8.1",
    "howler": "^2.2.3",
    "ip": "^1.1.5",
    "passport": "^0.5",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^2.0.0",
    "passport-spotify": "^2.0.0",
    "qrcode": "^1.4.4",
    "session-file-store": "^1.5.0",
    "socket.io": "^2.4.1",
    "spotify-web-api-node": "^5.0.2",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vue-socket.io": "^2.1.1-b",
    "vuex": "^3.6.2",
    "write-file-atomic": "^3.0.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@types/app-root-path": "^1.2.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/express-socket.io-session": "^1.3.6",
    "@types/ip": "^1.1.0",
    "@types/jest": "^27.4.1",
    "@types/minimist": "^1.2.2",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.20.47",
    "@types/passport": "^0.4.6",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-spotify": "^2.0.1",
    "@types/session-file-store": "^1.2.2",
    "@types/socket.io": "^1.4.42",
    "@types/spotify-api": "^0.0.14",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vue/babel-preset-app": "^5.0.4",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "concurrently": "^4.1.2",
    "electron": "^7.3.3",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "minimist": "^1.2.5",
    "npm-watch": "^0.4.0",
    "prettier": "^2.6.2",
    "sass": "^1.42.1",
    "sass-loader": "^7.3.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "vue-eslint-parser": "^8.3.0",
    "vue-template-compiler": "^2.6.12"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,html,vue,css,md}": "prettier --write"
  }
}
